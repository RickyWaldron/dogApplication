html
	head
		title Dog Matches
		script(src="https://code.jquery.com/jquery-3.2.1.js"
			integrity="sha256-DZAnKJ/6XZ9si04Hgrsxu/8s717jcIzLy3oi35EouyE="
			crossorigin="anonymous"
			)
		link(rel='stylesheet', type='text/css', href='css/style.css')
		link(rel='stylesheet', type='text/css', href='css/match.css')
		link(href="https://fonts.googleapis.com/css?family=Open+Sans|Oswald" rel="stylesheet")

	body
		div(class="site-container1")
			div(class="site-pusher1")
				include nav.pug
				div(class="container")
					div(class="filling")
					h2(class="h2") Find your match
					div(class="containerMatchGallery")
						- for (var i=0; i < allMatches[0].rows.length; i++)
							div(class="oneMatch")
								div(class="match")
									img(class="matchPicture", src='uploads/'+allMatches[0].rows[i].picture)
								div(class="match" id="namedog") #{allMatches[0].rows[i].dogname}
								div(class="match" id="aboutdog") #{allMatches[0].rows[i].aboutdog}
					div(class="containerMap")
						img(class="map", src="/images/map.png")